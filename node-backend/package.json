{
  "name": "node-backend",
  "version": "0.1.0",
  "description": "Backend written in Node",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=7.6.0"
  },
  "scripts": {
    "start": "node cluster.js",
    "dev": "cross-env NODE_PATH=. NODE_ENV=development nodemon",
    "debug": "cross-env NODE_PATH=. NODE_ENV=development nodemon --inspect",
    "test": "npm run test:all",
    "test:all": "npm run test:unit && npm run test:features",
    "test:unit": "cross-env NODE_PATH=. NODE_ENV=test mocha --config test/.mocharc.unit.js",
    "test:features": "cross-env NODE_PATH=. NODE_ENV=test mocha --config test/.mocharc.features.js",
    "coverage": "cross-env NODE_PATH=. NODE_ENV=test nyc mocha --config test/.mocharc.unit.js",
    "lint": "eslint {src,test,config}/**/*.js",
    "sequelize": "cross-env NODE_PATH=. sequelize",
    "console": "cross-env NODE_PATH=. node src/interfaces/console/index.js",
    "heroku-postbuild": "NODE_ENV=production NODE_PATH=. sequelize db:migrate --url=$DATABASE_URL",
    "pm2": "pm2",
    "cleanup": "node scripts/cleanup.js"
  },
  "repository": {},
  "author": "Florentin BÃ¼rvenich",
  "license": "MIT",
  "dependencies": {
    "awilix": "^4.2.6",
    "awilix-express": "^3.0.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "cross-env": "^7.0.2",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "express": "^4.15.2",
    "express-status-monitor": "^1.3.3",
    "http-status": "^1.0.1",
    "log4js": "^6.3.0",
    "method-override": "^3.0.0",
    "morgan": "^1.8.1",
    "pg": "^8.5.0",
    "pm2": "^4.5.0",
    "postgresql": "^0.0.1",
    "random": "^2.2.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "structure": "^2.0.1",
    "swagger-ui-express": "^2.0.14"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-change": "^2.1.2",
    "chance": "^1.0.6",
    "dirty-chai": "^2.0.1",
    "factory-girl": "^5.0.4",
    "listr": "^0.14.3",
    "mocha": "^8.2.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.2.1",
    "replace-in-file": "^2.5.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2"
  }
}
