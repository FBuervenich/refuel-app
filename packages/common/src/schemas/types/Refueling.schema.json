{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.refuel-app.fbuervenich.de/types/Refueling.schema.json",
  "title": "Refueling",
  "type": "object",
  "required": ["id", "litres", "price", "pricePerLitre", "madeAt"],
  "properties": {
    "id": { "type": "number" },
    "litres": { "type": "number" },
    "price": { "type": "number" },
    "pricePerLitre": { "type": "number" },
    "totalKilometers": { "type": "number" },
    "dayKilometers": { "type": "number" },
    "fullTank": { "type": "boolean" },
    "madeAt": { "type": "string", "format": "date-time" },
    "userId": { "type": "string" }
  },
  "additionalProperties": false
}
