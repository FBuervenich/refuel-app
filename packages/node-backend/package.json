{
  "name": "node-backend",
  "version": "0.1.0",
  "description": "Backend written in Node",
  "main": "dist/node-backend/index.js",
  "private": true,
  "engines": {
    "node": ">=7.6.0"
  },
  "scripts": {
    "start": "node dist/cluster.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"#3178c6.bold,#68a063.bold\" \"npm run watch\" \"cross-env NODE_PATH=./dist NODE_ENV=development nodemon -r source-map-support/register\"",
    "debug": "npm run dev --inspect",
    "build": "cross-env NODE_PATH=./ tsc",
    "test": "npm run test:all",
    "test:all": "npm run test:unit && npm run test:features",
    "test:unit": "cross-env NODE_PATH=./dist NODE_ENV=test mocha --config test/.mocharc.unit.js",
    "test:features": "cross-env NODE_PATH=./dist NODE_ENV=test mocha --config test/.mocharc.features.js",
    "coverage": "cross-env NODE_PATH=./dist NODE_ENV=test nyc mocha --config test/.mocharc.unit.js",
    "lint": "eslint {src,test,config}/**/*.{js,ts}",
    "sequelize": "cross-env NODE_PATH=./dist sequelize",
    "console": "cross-env NODE_PATH=./dist node src/interfaces/console/index.js",
    "heroku-postbuild": "npm run build && NODE_ENV=production NODE_PATH=./dist sequelize db:migrate --url=$DATABASE_URL",
    "pm2": "pm2",
    "cleanup": "node scripts/cleanup.js",
    "watch": "tsc -w"
  },
  "repository": {},
  "author": "Florentin BÃ¼rvenich",
  "license": "MIT",
  "dependencies": {
    "@types/chance": "^1.1.1",
    "awilix": "^4.2.6",
    "awilix-express": "^4.0.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.15.2",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "express-status-monitor": "^1.3.3",
    "http-status": "^1.0.1",
    "jwks-rsa": "^2.0.2",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "method-override": "^3.0.0",
    "morgan": "^1.8.1",
    "neverthrow": "^4.2.1",
    "pg": "^8.5.0",
    "pm2": "^4.5.0",
    "postgresql": "^0.0.1",
    "random": "^3.0.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "source-map-support": "^0.5.19",
    "structure": "^2.0.1",
    "swagger-ui-express": "^4.1.5"
  },
  "devDependencies": {
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.2",
    "@types/validator": "^13.1.3",
    "chai": "^4.1.2",
    "chai-change": "^2.1.2",
    "chance": "^1.0.6",
    "dirty-chai": "^2.0.1",
    "factory-girl": "^5.0.4",
    "listr": "^0.14.3",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "replace-in-file": "^6.1.0",
    "supertest": "^6.0.1",
    "supertest-as-promised": "^4.0.2",
    "typescript": "^4.2.4"
  }
}
